if 	IF
while 	WHILE
[a-z][a-z]*	ID
break 	BR
continue	CONT
return	RET
void	VOID
int	INT
char	CHAR
float	FLOAT
+	PLUS
\*	MUL
\-	MINUS
/	DIV
\|	OR
\=	ASSIGNMENT
\!	NOT
[0-9][0-9]*	CONST_INT
[0-9]*\.[0-9][0-9]*	CONST_FLOAT
[0-9]*E(+|-|#)[0-9][0-9]*	CONST_FLOAT
'.'	LIT
".*"	STR
\(	LPAREN
\)	RPAREN
{	LBRACE
}	RBRACE
